        <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.5.3/dist/umd/popper.min.js"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
        <script src="https://unpkg.com/typed.js@2.0.16/dist/typed.umd.js"></script>
        <script>
            var typingEffect = new Typed(".multitext", {
                strings: ["لبناء مستقبل مزدهر","لتحقيق أهدافك","لبناء نجاحك"],
                loop: true,
                typeSpeed: 90,
                backSpeed: 80,
            })
        </script>

        <script>
            document.addEventListener('DOMContentLoaded', () => {
    const members = [
        { name: 'سهيلة', role: 'هندسة برمجيات', info: 'نحن مجموعة من طالبات هندسة البرمجيات...' },
        { name: 'ليان', role: 'هندسة برمجيات', info: 'نحن مجموعة من طالبات هندسة البرمجيات...' },
        { name: 'رشا', role: 'هندسة برمجيات', info: 'نحن مجموعة من طالبات هندسة البرمجيات...' },
        { name: 'أماني', role: 'هندسة برمجيات', info: 'نحن مجموعة من طالبات هندسة البرمجيات...' }
    ];

    let index = 0;

    const updateCard = () => {
        document.querySelector('.member-name').textContent = members[index].name;
        document.querySelector('.member-role').textContent = members[index].role;
        document.querySelector('.member-info').textContent = members[index].info;
    };

    document.querySelector('.prev-btn').addEventListener('click', () => {
        index = (index - 1 + members.length) % members.length;
        updateCard();
    });

    document.querySelector('.next-btn').addEventListener('click', () => {
        index = (index + 1) % members.length;
        updateCard();
    });

    updateCard();
});

        </script>
</body>

</html>